<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/overlay_map_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:maxWidth="300dp"
    android:paddingEnd="5dp"
    android:paddingStart="10dp"
    android:maxHeight="300dp"
    android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textview_map_settings"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="left"
            android:elegantTextHeight="true"
            android:gravity="center|start"
            android:paddingRight="5dp"
            android:text="Map Settings"
            android:textAlignment="viewStart"
            android:textAllCaps="false"
            android:textColor="#FFFFFF"
            android:textDirection="ltr"
            android:textSize="16sp"
            android:textStyle="bold"
            app:autoSizeTextType="uniform"
            tools:ignore="MissingConstraints" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="200dp"
            android:orientation="vertical"
            android:scrollbars="vertical"
            android:id="@+id/incident_recycler_view"
            android:paddingEnd="5dp"
            >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                >


                <TextView
                    android:id="@+id/textview_map_settings_show"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left"
                    android:elegantTextHeight="true"
                    android:gravity="center|start"
                    android:paddingRight="5dp"
                    android:text="Show..."
                    android:textAlignment="viewStart"
                    android:textAllCaps="false"
                    android:textColor="#FFFFFF"
                    android:textDirection="ltr"
                    android:textSize="12sp"
                    android:textStyle="bold"
                    app:autoSizeTextType="uniform" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_1"
                    android:key="type_1"
                    android:defaultValue="true"
                    android:text="... NORMAL type mon (⦿)"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    app:map_preference_key="type_1"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_2"
                    android:key="type_2"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... FIGHTING type mon (仝)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_2"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_3"
                    android:key="type_3"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... FLYING type mon (ত)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_3"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_4"
                    android:key="type_4"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... POISON type mon (৶)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_4"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_5"
                    android:key="type_5"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... GROUND type mon (⏚)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_5"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_6"
                    android:key="type_6"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... ROCK type mon (ᨏ)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_6"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_7"
                    android:key="type_7"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... BUG type mon (४)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_7"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_8"
                    android:key="type_8"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... GHOST type mon (९)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_8"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_9"
                    android:key="type_9"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... STEEL type mon (◎)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_9"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_10"
                    android:key="type_10"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... FIRE type mon (ঌ)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_10"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_11"
                    android:key="type_11"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... WATER type mon (〜)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_11"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_12"
                    android:key="type_12"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... GRASS type mon (∭)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_12"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_13"
                    android:key="type_13"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... ELECTRIC type mon (ᔪ)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_13"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_14"
                    android:key="type_14"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... PSYCHIC type mon (७)"
                    android:textColor="#FFFFFF"
                    android:textStyle="bold"
                    android:textSize="12sp"
                    app:map_preference_key="type_14"
                    />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_15"
                    android:key="type_15"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... ICE type mon (❀)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_15"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_16"
                    android:key="type_16"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... DRAGON type mon (ઙ)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_16"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_17"
                    android:key="type_17"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... DARK type mon (◐)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_17"
                    android:textStyle="bold"
                    android:textSize="12sp" />

                <com.mad.pogoenhancer.overlay.elements.mapSettings.MapSettingSwitch
                    android:id="@+id/type_18"
                    android:key="type_18"
                    android:defaultValue="true"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:text="... FAIRY type mon (꩓)"
                    android:textColor="#FFFFFF"
                    app:map_preference_key="type_18"
                    android:textStyle="bold"
                    android:textSize="12sp" />

            </LinearLayout>
        </ScrollView>
    </LinearLayout>


</androidx.constraintlayout.widget.ConstraintLayout>